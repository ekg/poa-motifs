require(ggplot2)
require(tidyverse)
x <- read.delim('tomtom_merged_clusts_euclidean.aligned.tsv')
summary(x)
ggplot(x, aes(x=identity, y=haplotype.score)) + geom_text()

require(ggrepel)
ggplot(x, aes(x=identity, y=haplotype.score)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, size=length(sequence))) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, size=sequence)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score)) + geom_point()
ggplot(x, aes(x=haplotype.score)) + geom_point()
ggplot(x, aes(x=haplotype.score)) + geom_density()
x <- read.delim('tomtom_merged_clusts_euclidean.fwd+rev.aligned.tsv')
ggplot(x, aes(x=haplotype.score, color=group)) + geom_density()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_jitter()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density2d()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d()
ggplot(x, aes(x=identity, y=-haplotype.score, color=group)) + geom_density_2d()
?geom_density_2d
ggplot(x, aes(x=identity, y=-haplotype.score, color=group)) + geom_density_2d(h=c(1,1))
ggplot(x, aes(x=identity, y=-haplotype.score, color=group)) + geom_density_2d(h=c(10,10))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(10,10))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(10,1))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(1,10))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,1)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,5)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.01,5)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,5)) + geom_point()
savehistory()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_point()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_jitter()
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_point(alpha=I(1/3))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_point(alpha=I(1/2))
ggplot(x, aes(x=identity, y=haplotype.score, color=group)) + geom_density_2d(h=c(0.1,10)) + geom_point(alpha=I(1/3))
ggsave('tomtom_merged_clusts_euclidean.fwd+rev.aligned.tsv.pdf')
ggsave('tomtom_merged_clusts_euclidean.fwd+rev.aligned.tsv.png')
savehistory()
